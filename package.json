{
  "name": "queen-mq",
  "version": "0.1.6",
  "type": "module",
  "description": "High-performance message queue backed by PostgreSQL",
  "main": "src/client/index.js",
  "scripts": {
    "start": "node src/server.js",
    "start:cluster": "node src/cluster-server.js",
    "build:webapp": "cd webapp && npm install && npm run build",
    "copy:webapp": "rm -rf src/webapp-dist && cp -r webapp/dist src/webapp-dist",
    "prepublishOnly": "npm run build:webapp && npm run copy:webapp",
    "publish:public": "npm run build:webapp && npm run copy:webapp && npm publish --access public",
    "test:produce": "node test.js produce",
    "test:consume": "node test.js consume",
    "test": "node src/test/test.js",
    "test:core": "node src/test/core-features-test.js",
    "test:comprehensive": "node src/test/comprehensive-test.js",
    "test:edge": "node src/test/comprehensive-edge-cases-test.js",
    "test:advanced": "node src/test/advanced-scenarios-test.js",
    "test:all": "npm run test && npm run test:edge && npm run test:advanced",
    "test:quick": "npm run test:core",
    "test:lease": "node src/test/test-lease-expiration.js",
    "test:lease:simple": "node test-lease-simple.js",
    "test:client": "node test-client.js",
    "test:simple": "node test-simple.js",
    "init": "node init-db.js"
  },
  "files": [
    "src/",
    "init-db.js",
    "LICENSE.md",
    "README.md"
  ],
  "dependencies": {
    "axios": "^1.12.2",
    "chart.js": "^4.5.1",
    "pg": "^8.13.3",
    "uuid": "^13.0.0",
    "uWebSockets.js": "github:uNetworking/uWebSockets.js#v20.52.0",
    "ws": "^8.18.3"
  },
  "engines": {
    "node": ">=22.0.0"
  },
  "author": "Smartness",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/smartpricing/queen"
  },
  "keywords": [
    "message-queue",
    "queue",
    "broker",
    "message-queue-system",
    "fifo"
  ]
}
